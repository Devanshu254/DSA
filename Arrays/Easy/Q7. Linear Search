import java.util.*;

public class Main {
    public static int linearSearch(int[] arr, int n, int num) {
      for(int i=0;i<n;i++) {
        if(arr[i] == num) {
          return i; 
        }
      }
      return -1;
    }
    public static void main(String[] args) {
      int[] arr = {5,4,3,2,1};
      int n = arr.length;
      int num = 5;
      int z = linearSearch(arr,n, num);
      System.out.println(z);
  }
}
